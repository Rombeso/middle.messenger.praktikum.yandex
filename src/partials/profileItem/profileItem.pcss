.dataItem {
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 100%;
    height: 35px;
    border-bottom: 1px solid var(--grey);

    &__title {
        font-size: 13px;
        color: var(--second-text);
        font-weight: 500;
    }

    &__content {
        font-size: 13px;
        font-weight: 500;
    }
}